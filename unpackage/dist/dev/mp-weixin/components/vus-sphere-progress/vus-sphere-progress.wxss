	/* 动画 */
.vus-progress-sphere .vus-progress-sphere-anim.data-v-a3592524 {
		-webkit-animation: VusProgressWater-data-v-a3592524 10s infinite forwards linear;
		        animation: VusProgressWater-data-v-a3592524 10s infinite forwards linear;
		-webkit-perspective: 1000;
		-webkit-backface-visibility: hidden;
		-webkit-transform-style: preserve-2d;
}
