	/* 气泡 */
.vus-progress-sphere .vus-progress-sphere-bubble.data-v-a3592524 {
		width: 12rpx;
		height: 12rpx;
		position: absolute;
		left: calc(50% - 6rpx);
		bottom: 0;
		z-index: 4;
		opacity: 0;
		/* background-color: rgba(255, 255, 255, .3); */
		border-radius: 50%;
}
