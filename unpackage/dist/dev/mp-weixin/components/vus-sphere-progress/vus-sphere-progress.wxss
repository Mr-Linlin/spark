.vus-progress-sphere-custom.data-v-a3592524,
	.vus-progress-sphere-y0.data-v-a3592524,
	.vus-progress-sphere-y1.data-v-a3592524,
	.vus-progress-sphere-y2.data-v-a3592524,
	.vus-progress-sphere-y3.data-v-a3592524,
