	.vus-progress-sphere-y8.data-v-a3592524,
	.vus-progress-sphere-y9.data-v-a3592524,
	.vus-progress-sphere-y10.data-v-a3592524,
	
		{
		width: 200rpx;
		height: 200rpx;
		transition: all 1s ease;
}
.vus-progress-sphere.data-v-a3592524 {
		width: 100%;
		height: 100%;
		position: relative;
		overflow: hidden;
		border-radius: 50%;
		box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.2);
		transition: all 1s ease;
}
.vus-progress-sphere .vus-progress-sphere-inner.data-v-a3592524 {
		width: inherit;
		height: inherit;
		position: relative;
		overflow: hidden;
		background-color: rgba(133, 133, 133, 1);
		transition: all 1s ease;
}
.vus-progress-sphere-water.data-v-a3592524 {
		width: 200%;
		height: 200%;
		position: absolute;
		left: -50%;
		border-radius: 40%;
		transition: all 1s ease;
}
.vus-progress-sphere .vus-progress-sphere-water.data-v-a3592524::before,
	.vus-progress-sphere .vus-progress-sphere-water.data-v-a3592524::after {
		width: 100%;
		height: 100%;
		background-image: inherit;
		content: '';
		position: absolute;
		left: 0;
		top: 0;
		border-radius: inherit;
}
.vus-progress-sphere .vus-progress-sphere-water.data-v-a3592524::before {
		opacity: 0.4;
		-webkit-transform: rotate(15deg) scale(1.05);
		        transform: rotate(15deg) scale(1.05);
}
.vus-progress-sphere .vus-progress-sphere-water.data-v-a3592524::after {
		opacity: 0.2;
		-webkit-transform: rotate(-25deg) scale(1.07);
		        transform: rotate(-25deg) scale(1.07);
}
	/* 高光 */
.vus-progress-sphere .vus-progress-sphere-glare.data-v-a3592524 {
		width: 200%;
		height: 200%;
		position: absolute;
		top: -140%;
		left: -120%;
		z-index: 6;
		/* background-color: rgba(255, 255, 255, 0.1); */
		-webkit-transform: rotate(0deg);
		        transform: rotate(0deg);
		border-radius: 50%;
		transition: all 1s ease;
}
